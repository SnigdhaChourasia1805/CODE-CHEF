#include <iostream>
using namespace std;

int main(){
    int t,a,b,x,y;
    cin>>t;
    while(t--){
        cin>>a>>b>>x>>y;
        if(a==b){
            cout<<"YES\n";
        }
        else if(a<b){
            if(b-a>x){
                cout<<"NO\n";
            }
            else{
                cout<<"YES\n";
            }
        }
        else{
            if(a-b>y){
                cout<<"NO\n";
            }
            else{
                cout<<"YES\n";
            }
        }
    }
}
