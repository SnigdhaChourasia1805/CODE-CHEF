#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    for(int i; i<t; i++)
    {
        int x;
        cin>>x;
        if(x%4==0)
        {
            cout<<"North"<<endl;
        }
        else if(x%4==1)
        {
            cout<<"East"<<endl;
        }
        else if(x%4==2)
        {
            cout<<"South"<<endl;
        }
        else if(x%4==3)
        {
            cout<<"West"<<endl;
        }
    }
	return 0;
}
