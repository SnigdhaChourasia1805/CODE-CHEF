#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int i,t;
	float x1,x2,x3,v1,v2,a=0,b=0;
	cin>>t;
	for(i=1;i<=t;i++)
	{
	    cin>>x1>>x2>>x3>>v1>>v2;
	    a=(x3-x1)/v1;
	    b=(x2-x3)/v2;
	    if(a==b)
	    {
	        cout<<"Draw"<<endl;
	    }
	    else if(a>b)
	    {
	        cout<<"Kefa"<<endl;
	    }
	    else if(a<b)
	    {
	        cout<<"Chef"<<endl;
	    }
	}
	return 0;
}
