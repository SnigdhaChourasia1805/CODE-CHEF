#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    long long count=0;
	    for(int i=0;i<=8;i++){
	        if((i>=a && i<=b) || (i>=c && i<=d)){
	            count++;
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}
