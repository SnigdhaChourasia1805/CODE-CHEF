#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int a,b,c;
        cin>>a>>b>>c;
        if((a>b&&a<c) || (a>c&&a<b)) 
        cout<<a;
        else if((b>c&&b<a) || (b>a&&b<c)) 
        cout<<b;
        else 
        cout<<c;
        cout<<endl;
        
        
    }
	return 0;
}
