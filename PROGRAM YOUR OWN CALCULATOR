#include <bits/stdc++.h>
using namespace std;

int main() {
    int  a,b;
    cin>>a>>b;
    char c;
    cin>>c;
    if(c=='+'){
        cout<<a+b<<endl;
    }
    else if(c=='-'){
        cout<<a-b<<endl;
    }
    else if(c=='*'){
        cout<<a*b<<endl;
    }
    else{
        if(a!=0)
        cout<<fixed<<setprecision(7)<<double(a)/b<<endl;
        else{
            cout<<0<<endl;
        }
    }
	
	return 0;
}
