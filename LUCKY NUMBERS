#include <iostream>
using namespace std;

int main() {
	int t,x,temp;
	cin>>t;
	while(t--)
	{
	    cin>>x;
	    while(x>0)
	    {
	        temp = x%10;
	        x=x/10;
	        if(temp==7)
	        {
	            cout<<"YES"<<endl;
	            break;
	        }
	        else if(x==0)
	        {
	            cout<<"NO"<<endl;
	        }
	        
	    }
	}
	return 0;
}
