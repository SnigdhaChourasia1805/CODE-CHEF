#include <iostream>
using namespace std;

int
main ()
{
  int t;
  cin >> t;
  for (int i = 0; i < t; i++)
    {

      float s, a, b, c;

      cin >> s >> a >> b >> c;
      float p = s + ((s * c) / 100);
      if (p >= a && p <= b)
	{
	  cout << "Yes" << endl;
	}
      else
	{
	  cout << "No" << endl;
	}
    }
  return 0;
}
