#include <iostream>
using namespace std;

int main() {
	int i,n,c=0,d=0;
	cin>>n;
	int a[n];
	for(i=0;i<n;i++)
	{
	    cin>>a[i];
	}
	for(i=0;i<n;i++)
	{
	    if(a[i]%2==0) 
	    {
	      c++;  
	    }
	    else 
	    {
	        d++;
	    }
	}
	if(c>d)
	{
	    cout<<"READY FOR BATTLE"<<endl;
	}
	else
	{
	    cout<<"NOT READY"<<endl;
	}
	return 0;
}
