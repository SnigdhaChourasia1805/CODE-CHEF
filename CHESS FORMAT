#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int a,b;
        cin>>a>>b;
        if((a+b)<3){
            cout<<1<<endl;
        }
        else if((a+b)<=10 && (a+b)>=3){
            cout<<2<<endl;
        }
        else if((a+b)<=60 && (a+b)>=11){
            cout<<3<<endl;
        }
        else{
            cout<<4<<endl;
        }
    }
} 
