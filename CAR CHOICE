
#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	double x1,x2,y1,y2;
	cin>>t;
	for(int i=0;i<t;i++)
	 {
	    cin>>x1>>x2>>y1>>y2;
	    if((x1/x2)<(y1/y2))
	     cout<<"1"<<endl;
	    else if((x1/x2)==(y1/y2))
	     cout<<"0"<<endl;
	    else 
         cout<<"-1"<<endl;
	}
	return 0;
}
