#include <iostream>
using namespace std;

int main() {
	int t,n,i;
	char s[100];
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>n;
	    for(i=0;i<n;i++)
	    {
	        cin>>s[i];
	        if(s[i]=='A')
	        s[i]='T';
	        else if(s[i]=='T')
	        s[i]='A';
	        
	        else if(s[i]=='C')
	        s[i]='G';
	        
	        else if(s[i]=='G')
	        s[i]='C';	          
	    }
	    for(i=0;i<n;i++)
	    cout<<s[i];
	    cout<<endl;
	}
	return 0;
}
