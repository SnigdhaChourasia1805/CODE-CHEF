#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    int x,y;
	    cin>>x>>y;
	    if(2*x>y*5){
	        cout<<"Chocolate"<<endl;
	    }
	    else if(5*y==2*x){
	        cout<<"Either"<<endl;
	    }
	    else{
	        cout<<"Candy"<<endl;
	    }
	    
	}
	return 0;
}
