#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    long long a1,a2;
	    cin>>a1>>a2;
	    long long b1,b2;
	    cin>>b1>>b2;
	    long long c1,c2;
	    cin>>c1>>c2;
	    int a=a1+a2;
	    int b=b1+b2;
	    int c=c1+c2;
	    int d=max(a,b);
	    int e=max(d,c);
	    cout<<e<<endl;
	}
	return 0;
}
