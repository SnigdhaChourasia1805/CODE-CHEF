#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    string s;
	    string t;
	    cin>>s>>t;
	    for(int i=0;i<5;i++){
	        if(s[i]==t[i]){
	            cout<<"G";
	        }
	        else{
	            cout<<"B";
	        }
	    }
	    cout<<endl;
	    
	}
	return 0;
}
