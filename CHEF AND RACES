#include <iostream>
using namespace std;

int main() {
	int t,x,y,a,b;
    cin >> t;
    while(t--)
    {
        int f=0;
        std::cin >> x>>y>>a>>b;
        if((x==a||x==b) && (y==a || y==b))  
        cout << 0 << endl;
        else if((x!=a && x!=b) && (y!=a && y!=b)) 
        cout << 2 << endl;
        else cout << 1 << endl;
    }
	return 0;
}
