#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    int x,y,a,b,c;
	    cin>>x>>y>>a>>b>>c;
	    if((x*y)<10){
	        cout<<0<<endl;
	    }
	    else if((x*y)>=10 && (x*y)<21){
	        cout<<a<<endl;
	    }
	    else if((x*y)>=21 && ((x*y)<42)){
	        cout<<b<<endl;
	    }
	    else{
	        cout<<c<<endl;
	    }
	}
	return 0;
}
