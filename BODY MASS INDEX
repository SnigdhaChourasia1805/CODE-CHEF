#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int m,h,a;
        cin>>m>>h;
        a=h*h;
        if((m/a)<=18){
            cout<<1<<endl;
        }
        else if((m/a)>=19 && (m/a)<=24){
            cout<<2<<endl;
        }
        else if((m/a)>=25 && (m/a)<=29){
            cout<<3<<endl;
        }
        else{
            cout<<4<<endl;
        }
        
    }
}
