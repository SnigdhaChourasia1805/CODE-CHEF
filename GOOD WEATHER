#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        int array[7];
        int count=0;
        for(int i=0;i<7;i++){
            cin>>array[i];
            if(array[i] == 1){
                count++;
            }
        }
        if(count>=4){
            cout<<"yes"<<endl;
        }
        else{
            cout<<"no"<<endl;
        }
    }
    return 0;
}
