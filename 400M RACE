#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    int x,y,z;
	    cin>>x>>y>>z;
	    int a=min(x,min(y,z));
	    if(a==x){
	        cout<<"ALICE"<<endl;
	    }
	    else if(a==y){
	        cout<<"BOB"<<endl;
	    }
	    else{
	        cout<<"CHARLIE"<<endl;
	    }
	}
	return 0;
}
